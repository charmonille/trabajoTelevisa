/**INICIAL*/	
	
					function secondStage1(){
						
							var secondStage = document.getElementById("secondStage");
					TweenLite.to(secondStage,.4, {top:"0%",
				                       ease:Elastic.easeInOut});
						
						/*LAMPARA*/
							var isMiddleElement12=document.getElementById("isMiddleElement12");
							TweenLite.to(isMiddleElement12, .59, {bottom:"36%",delay:0.5});
						/*VENTANA*/	
							var isMiddleElement1=document.getElementById("isMiddleElement1");
							TweenLite.to(isMiddleElement1, .45, {top:"0%",delay:0.5});
						/*MESA*/
							var isMiddleElement2=document.getElementById("isMiddleElement2");
							TweenLite.to(isMiddleElement2, .59, {top:"0%",delay:0.5});
						/*TV*/	
							var isMiddleElement3=document.getElementById("isMiddleElement3");
							TweenLite.to(isMiddleElement3, .9, {top:"0%",delay:0.5});
						/*COMIDA PERRO*/
							var isMiddleElement11=document.getElementById("isMiddleElement11");
							TweenLite.to(isMiddleElement11, 1, {bottom:"32%",delay:0.5});
						/*PERRO*/
							var isMiddleElement5=document.getElementById("isMiddleElement5");
							TweenLite.to(isMiddleElement5, 1, {top:"23%",delay:0.7});
						/*SHINING*/	
							var isMiddleElement6=document.getElementById("isMiddleElement6");
							TweenLite.to(isMiddleElement6, 1, {top:"-4%",delay:0.9});
						/*PERRO*/
							var isMiddleElement9=document.getElementById("isMiddleElement9");
							TweenLite.to(isMiddleElement9, .7, {bottom:"19%",delay:0.5});
						/*CUADROS1*/	
							var isMiddleElement4=document.getElementById("isMiddleElement4");
							TweenLite.to(isMiddleElement4, 1.4, {top:"0%",delay:0.2});
						/*CUADROS2*/
							var isMiddleElement8=document.getElementById("isMiddleElement8");
							TweenLite.to(isMiddleElement8, .8, {top:"35%",delay:0.4});
						
						
						/*PASTO*/	
							var secondBgElement2=document.getElementById("secondBgElement2");
							TweenLite.to(secondBgElement2, .0009, {bottom:"0%",delay:0.5});	
						/**/	
				}
	
/*TERMINA INICIAL*/	

